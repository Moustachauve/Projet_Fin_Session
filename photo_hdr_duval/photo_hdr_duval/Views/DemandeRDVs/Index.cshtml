@model IEnumerable<photo_hdr_duval.Models.RDV>

@{
    ViewBag.Title = "Index";

}

<h2>Liste de Rendez-vous @Html.ActionLink("Demander un rendez-vous", "Create", "DemandeRDVs", null, new { @class = "btn btn-success pull-right" })</h2>

<table class="table table-hover table-striped">
    
    @Html.Partial("../Shared/IndexTableHeaderRDVs")

	<tbody>
		@foreach (var item in Model)
		{
			<tr>
                @Html.Partial("../Shared/IndexTableDataRDVs", item)
				<td class="text-right">
					<div class="btn-group">
						@Html.ActionLink("Détails", "Details", new { id = item.RDVID }, new { @class = "btn btn-primary btn-sm" })
						@Html.ActionLink("Modifier", "Edit", new { id = item.RDVID }, new { @class = "btn btn-primary btn-sm" })
					</div>
				</td>
			</tr>
		}
	</tbody>



</table>
